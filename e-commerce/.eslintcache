encyFormat from \"react-currency-format\";\r\nimport axios from \"../axios\";\r\n\r\nconst Payment = () => {\r\n  const [{ basket, user }, dispatch] = useStateValue();\r\n\r\n  const history = useHistory();\r\n\r\n  const stripe = useStripe();\r\n  const elements = useElements();\r\n\r\n  const [succeeded, setSucceeded] = useState(false);\r\n  const [processing, setProcessing] = useState(\"\");\r\n  const [error, setError] = useState(\"\");\r\n  const [disabled, setDisabled] = useState(true);\r\n  const [clientSecret, setClientSecret] = useState(true);\r\n\r\n  useEffect(() => {\r\n    // Generate the special stripe secret key which allows to charge a customer\r\n    const getClientSecret = async () => {\r\n      const response = await axios({\r\n        method: \"post\",\r\n        // Stripe expects the total currency in subunits\r\n        url: `/payments/create?total=${getBasketTotalPrice(basket) * 100}`,\r\n      });\r\n      setClientSecret(response.data.clientSecret);\r\n    };\r\n\r\n    getClientSecret();\r\n  }, [basket]);\r\n\r\n  const handleSubmit = async (e) => {\r\n    e.preventDefault();\r\n    setProcessing(true);\r\n\r\n    const payload = await stripe\r\n      .confirmCardPayment(clientSecret, {\r\n        payment_method: {\r\n          card: elements.getElement(CardElement),\r\n        },\r\n      })\r\n      .then(({ paymentIntent }) => {\r\n        db.collection(\"users\")\r\n          .doc(user?.uid)\r\n          .collection(\"orders\")\r\n          .doc(paymentIntent.id)\r\n          .set({\r\n            basket: basket,\r\n            amount: paymentIntent.amount,\r\n            created: paymentIntent.created,\r\n          });\r\n\r\n        setSucceeded(true);\r\n        setError(null);\r\n        setProcessing(false);\r\n\r\n        dispatch({\r\n          type: \"EMPTY_BASKET\",\r\n        });\r\n        history.replace(\"/orders\");\r\n      });\r\n  };\r\n\r\n  const handleChange = (e) => {\r\n    // Listen to changes in Card element\r\n    setDisabled(e.empty);\r\n    // Display any error as the customer types their card details\r\n    setError(e.error ? e.error.message : \"\");\r\n  };\r\n  return (\r\n    <div className=\"payment\">\r\n      <div className=\"payment-container\">\r\n        <h1>\r\n          Checkout (<Link to=\"/checkout\"> {basket?.length} items </Link>)\r\n        </h1>\r\n        <div className=\"payment-section\">\r\n          <div className=\"payment-title\">\r\n            <h3>Deliviry Adress</h3>\r\n          </div>\r\n          <div className=\"payment-adress\">\r\n            <p>{user?.email}</p>\r\n            <p>Rishon</p>\r\n            <p>Kapach</p>\r\n          </div>\r\n        </div>\r\n        <div className=\"payment-section\">\r\n          <div className=\"payment-title\">\r\n            <h3>Review items and deleivry</h3>\r\n          </div>\r\n          <div className=\"payment-items\">\r\n            {basket.map((basketItem) => (\r\n              <CheckoutProduct\r\n                id={basketItem.id}\r\n                title={basketItem.title}\r\n                image={basketItem.image}\r\n                price={basketItem.price}\r\n                rating={basketItem.rating}\r\n              />\r\n            ))}\r\n          </div>\r\n        </div>\r\n\r\n        <div className=\"payment-section\">\r\n          <div className=\"payment-title\">\r\n            <h3>Payment method</h3>\r\n          </div>\r\n          <div className=\"payment-details\">\r\n            <form onSubmit={handleSubmit}>\r\n              <CardElement onChange={handleChange} />\r\n\r\n              <div className=\"payment-priceContainer\">\r\n                <CurrencyFormat\r\n                  renderText={(value) => (\r\n                    <>\r\n                      <h3>Order Total: {value}</h3>\r\n                    </>\r\n                  )}\r\n                  decimalScale={2}\r\n                  value={getBasketTotalPrice(basket)}\r\n                  displayType={\"text\"}\r\n                  thousandSeparator={true}\r\n                  prefix={\"$\"}\r\n                />\r\n                <button disabled={processing || disabled || succeeded}>\r\n                  <span>{processing ? <p>Processing</p> : \"Buy Now\"}</span>\r\n                </button>\r\n              </div>\r\n\r\n              {error && <div>{error}</div>}\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Payment;\r\n","C:\\Users\\Almog Cohen\\e-commerce\\src\\Components\\axios.js",[],"C:\\Users\\Almog Cohen\\e-commerce\\src\\Components\\Orders\\Orders.js",["90","91"],"C:\\Users\\Almog Cohen\\e-commerce\\src\\Components\\Orders\\Order.js",[],{"ruleId":"92","severity":1,"message":"93","line":20,"column":10,"nodeType":"94","messageId":"95","endLine":20,"endColumn":12},{"ruleId":"96","severity":1,"message":"97","line":42,"column":6,"nodeType":"98","endLine":42,"endColumn":8,"suggestions":"99"},{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","severity":1,"message":"105","line":11,"column":28,"nodeType":"106","messageId":"107","endLine":11,"endColumn":36},{"ruleId":"104","severity":1,"message":"108","line":1,"column":17,"nodeType":"106","messageId":"107","endLine":1,"endColumn":25},{"ruleId":"104","severity":1,"message":"109","line":8,"column":10,"nodeType":"106","messageId":"107","endLine":8,"endColumn":15},{"ruleId":"104","severity":1,"message":"105","line":8,"column":28,"nodeType":"106","messageId":"107","endLine":8,"endColumn":36},{"ruleId":"104","severity":1,"message":"105","line":8,"column":22,"nodeType":"106","messageId":"107","endLine":8,"endColumn":30},{"ruleId":"104","severity":1,"message":"110","line":6,"column":12,"nodeType":"106","messageId":"107","endLine":6,"endColumn":18},{"ruleId":"111","severity":1,"message":"112","line":17,"column":7,"nodeType":"113","endLine":17,"endColumn":60},{"ruleId":"111","severity":1,"message":"112","line